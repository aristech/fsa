{
  "name": "fsa",
  "version": "1.0.0",
  "description": "Field Service Application - A comprehensive field service management system",
  "private": true,
  "workspaces": [
    "apps/frontend",
    "apps/backend"
  ],
  "scripts": {
    "dev": "./start-dev.sh",
    "start": "./start-dev.sh",
    "build": "npm run build --workspace=apps/backend && npm run build --workspace=apps/frontend",
    "lint": "npm run lint --workspace=apps/frontend",
    "lint:fix": "npm run lint --workspace=apps/frontend -- --fix",
    "setup": "npm install && npm run setup --workspace=apps/backend",
    "clean": "rm -rf node_modules apps/*/node_modules apps/*/dist apps/*/.next"
  },
  "keywords": [
    "field-service",
    "management",
    "react",
    "nextjs",
    "nodejs",
    "fastify",
    "mongodb"
  ],
  "author": "FSA Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
